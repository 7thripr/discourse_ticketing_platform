       this.tag_3 = data.message.tag_3 ? \", \" + data.message.tag_3 : \"\";\n          this.attachments = data.message.attachments;\n        }\n        if (data.category == \"error\") {\n          this.flashMessage.error({\n            message: data.message\n          });\n        }\n      }).catch(error => {\n        this.$log.debug(`Error : ${error}`);\n        this.flashMessage.error({\n          message: \"Internal Server Error\"\n        });\n      });\n      // set variables\n      // show modal\n      this.showModalTicket();\n    },\n    showModalTicket() {\n      console.log(\"showModalTicket clicked\");\n      this.$bvModal.show(this.modal_id);\n    },\n    hideModalTicket() {\n      console.log(\"hideModalTicket clicked\");\n      this.$bvModal.hide(this.modal_id);\n    },\n    upvoteTicket() {\n      console.log(\"upvoteTicket clicked\");\n    },\n    deleteTicket() {\n      console.log(\"deleteTicket clicked\");\n    },\n    editTicket() {\n      console.log(\"editTicket clicked\");\n    }\n  },\n  computed: {}\n};","map":{"version":3,"names":["common","name","props","components","data","button_1_active","modal_id","ticket_id","priority","status","solution","resolved_by","resolved_on","tag_1","tag_2","tag_3","tags","attachments","created","console","log","created_on","created_by","title","description","votes","mounted","methods","getTicketDetails","fetch","TICKET_API","method","headers","web_token","$store","getters","get_web_token","user_id","get_user_id","then","response","json","$log","debug","category","message","flashMessage","error","catch","showModalTicket","$bvModal","show","hideModalTicket","hide","upvoteTicket","deleteTicket","editTicket","computed"],"sources":["src/components/TicketCard.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <b-container class=\"ticket-card-container\" @click=\"getTicketDetails\">\r\n      <!-- Stack the columns on mobile by making one full-width and the other half-width -->\r\n      <b-row class=\"row\">\r\n        <b-col class=\"col\" cols=\"12\" lg=\"5\" sm=\"12\"><i>Id: </i>{{ ticket_id.slice(0, 15) }}</b-col>\r\n        <b-col class=\"col\" cols=\"12\" lg=\"5\" sm=\"8\"><i>Created On: </i>{{ created_on }}</b-col>\r\n        <b-col class=\"col\" cols=\"12\" lg=\"2\" sm=\"4\"><i>Votes: </i>{{ votes }}</b-col>\r\n      </b-row>\r\n      <b-row class=\"row\">\r\n        <b-col class=\"col\" cols=\"12\" sm=\"10\" lg=\"11\">\r\n          <b-row class=\"row\">\r\n            <b-col class=\"col\" cols=\"12\"><b>Title: </b>{{ title.slice(0, 80) }}</b-col>\r\n          </b-row>\r\n          <b-row class=\"row\">\r\n            <b-col class=\"col\" cols=\"12\"><b>Description: </b>{{ description.slice(0, 200) }}</b-col>\r\n          </b-row>\r\n        </b-col>\r\n        <b-col class=\"col\" cols=\"12\" sm=\"2\" lg=\"1\">\r\n          <b-row class=\"row\">\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\">\r\n              <b-button\r\n                @click=\"upvoteTicket\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"upvote_disabled\"\r\n                ><b-icon\r\n                  icon=\"heart-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"success\"\r\n                ></b-icon>\r\n              </b-button>\r\n            </b-col>\r\n\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\"\r\n              ><b-button\r\n                @click=\"editTicket\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"edit_disabled\"\r\n                ><b-icon\r\n                  icon=\"pencil-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"primary\"\r\n                ></b-icon\r\n              ></b-button>\r\n            </b-col>\r\n\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\"\r\n              ><b-button\r\n                @click=\"deleteTicket\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"delete_disabled\"\r\n                ><b-icon\r\n                  icon=\"trash-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"danger\"\r\n                ></b-icon></b-button\r\n            ></b-col>\r\n          </b-row>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n\r\n    <b-modal :id=\"modal_id\" @cancel=\"hideModalTicket\" size=\"xl\" scrollable>\r\n      <template #modal-header=\"{ cancel }\">\r\n        <span style=\"font-size: 20px\">Ticket ID: {{ ticket_id }}</span>\r\n        <b-button size=\"sm\" variant=\"outline-danger\" @click=\"cancel()\"> Close </b-button>\r\n      </template>\r\n\r\n      <div class=\"d-block text-left\">\r\n        <h5 style=\"text-align: center\">Ticket Details</h5>\r\n        <!-- display as a table -->\r\n        <table>\r\n          <tr>\r\n            <td>Title:</td>\r\n            <td>{{ title }}</td>\r\n          </tr>\r\n          <tr>\r\n            <td>Description:</td>\r\n            <td>{{ description }}</td>\r\n          </tr>\r\n        </table>\r\n        <p>Title: {{ title }}</p>\r\n        <p>Description: {{ description }}</p>\r\n        <p>Status: {{ status }}</p>\r\n        <p>Priority: {{ priority }}</p>\r\n        <p>Tags: {{ tag_1 + tag_2 + tag_3 }}</p>\r\n        <p>Created on: {{ created_on }}</p>\r\n        <p>Creator's user id: {{ created_by }}</p>\r\n        <p>Votes: {{ votes }}</p>\r\n        <p>Resolver's user id: {{ resolved_by ? resolved_by : \"\" }}</p>\r\n        <p>Resolved on: {{ resolved_on == 0 ? \"\" : resolved_on }}</p>\r\n        <p>Solution: {{ solution }}</p>\r\n        <div>\r\n          <p>Attachments</p>\r\n          <div v-for=\"(attach, imageIndex) in attachments\" :key=\"imageIndex\">\r\n            <img :src=\"attach.attachment_loc\" class=\"img-fluid\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <template #modal-footer=\"{ cancel }\">\r\n        <b-button size=\"sm\" variant=\"danger\" @click=\"cancel()\"> Cancel </b-button>\r\n      </template>\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as common from \"../assets/common.js\";\r\n\r\nexport default {\r\n  name: \"TicketCard\",\r\n  props: [\r\n    \"ticket_id\",\r\n    \"created_on\",\r\n    \"created_by\",\r\n    \"title\",\r\n    \"description\",\r\n    \"votes\",\r\n    \"upvote_disabled\",\r\n    \"delete_disabled\",\r\n    \"edit_disabled\",\r\n  ],\r\n  components: {},\r\n  data() {\r\n    return {\r\n      button_1_active: false,\r\n      modal_id: \"bvmodal_\" + this.ticket_id,\r\n      priority: \"\",\r\n      status: \"\",\r\n      solution: \"\",\r\n      resolved_by: \"\",\r\n      resolved_on: \"\",\r\n      tag_1: \"\",\r\n      tag_2: \"\",\r\n      tag_3: \"\",\r\n      tags: \"\",\r\n      attachments: [],\r\n    };\r\n  },\r\n  created() {\r\n    console.log(\"ticket_id: \", this.ticket_id, typeof this.ticket_id);\r\n    console.log(\"created_on: \", this.created_on, typeof this.created_on);\r\n    console.log(\"created_on: \", this.created_by, typeof this.created_by);\r\n    console.log(\"title: \", this.title, typeof this.title);\r\n    console.log(\"description: \", this.description, typeof this.description);\r\n    console.log(\"votes: \", this.votes, typeof this.votes);\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    getTicketDetails() {\r\n      fetch(common.TICKET_API + `/${this.ticket_id}` + `/${this.created_by}`, {\r\n        method: \"GET\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          web_token: this.$store.getters.get_web_token,\r\n          user_id: this.$store.getters.get_user_id,\r\n        },\r\n      })\r\n        .then((response) => response.json())\r\n        .then((data) => {\r\n          this.$log.debug(`Success : ${data}`);\r\n          if (data.category == \"success\") {\r\n            // load data\r\n            this.priority = data.message.priority;\r\n            this.status = data.message.status;\r\n            this.solution = data.message.solution;\r\n            this.resolved_by = data.message.resolved_by;\r\n            this.resolved_on = data.message.resolved_on;\r\n            this.tag_1 = data.message.tag_1;\r\n            this.tag_2 = data.message.tag_2 ? \", \" + data.message.tag_2 : \"\";\r\n            this.tag_3 = data.message.tag_3 ? \", \" + data.message.tag_3 : \"\";\r\n\r\n            this.attachments = data.message.attachments;\r\n          }\r\n          if (data.category == \"error\") {\r\n            this.flashMessage.error({\r\n              message: data.message,\r\n            });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.$log.debug(`Error : ${error}`);\r\n          this.flashMessage.error({\r\n            message: \"Internal Server Error\",\r\n          });\r\n        });\r\n      // set variables\r\n      // show modal\r\n      this.showModalTicket();\r\n    },\r\n    showModalTicket() {\r\n      console.log(\"showModalTicket clicked\");\r\n      this.$bvModal.show(this.modal_id);\r\n    },\r\n    hideModalTicket() {\r\n      console.log(\"hideModalTicket clicked\");\r\n      this.$bvModal.hide(this.modal_id);\r\n    },\r\n    upvoteTicket() {\r\n      console.log(\"upvoteTicket clicked\");\r\n    },\r\n    deleteTicket() {\r\n      console.log(\"deleteTicket clicked\");\r\n    },\r\n    editTicket() {\r\n      console.log(\"editTicket clicked\");\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ticket-card-container {\r\n  /* border: 1px solid rgb(0, 0, 0); */\r\n  box-shadow: 2px 4px 5px 5px #dbdada;\r\n