on.FAQ_API, {\r\n      method: \"GET\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n        web_token: this.$store.getters.get_web_token,\r\n        user_id: this.user_id,\r\n      },\r\n    })\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        this.$log.debug(`Success : ${data}`);\r\n        if (data.category == \"success\") {\r\n          this.flashMessage.success({\r\n            message: \"User data retrieved.\",\r\n          });\r\n          this.faq_card_details = data.message;\r\n        }\r\n        if (data.category == \"error\") {\r\n          this.flashMessage.error({\r\n            message: data.message,\r\n          });\r\n        }\r\n      })\r\n      .catch((error) => {\r\n        this.$log.debug(`Error : ${error}`);\r\n        this.flashMessage.error({\r\n          message: \"Internal Server Error\",\r\n        });\r\n      });\r\n  },\r\n  mounted() {},\r\n  methods: {},\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAwBA,OAAAA,OAAA;AACA,OAAAC,UAAA;AACA,YAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH,UAAA;IAAAD;EAAA;EACAK,KAAA;IACA;MACAC,GAAA;MACAC,OAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,WAAA;MACAC,gBAAA;IACA;EACA;EACAC,QAAA;IACA,IAAAC,SAAA,QAAAL,MAAA,CAAAC,OAAA,CAAAK,aAAA;IACA,IAAAD,SAAA;MACA,KAAAP,GAAA;IACA;IACA,IAAAO,SAAA;MACA,KAAAP,GAAA;IACA;;IACA,IAAAO,SAAA;MACA,KAAAP,GAAA;IACA;;IACA,IAAAS,IAAA;MACAC,aAAA;IACA;IACA,IAAAC,MAAA;IACAA,MAAA,OAAAC,eAAA,CAAAH,IAAA,EAAAI,QAAA;IACAC,OAAA,CAAAC,GAAA,aAAAJ,MAAA;IAEAK,KAAA,CAAApB,MAAA,CAAAqB,OAAA;MACAC,MAAA;MACAC,OAAA;QACA;QACAC,SAAA,OAAAlB,MAAA,CAAAC,OAAA,CAAAkB,aAAA;QACApB,OAAA,OAAAA;MACA;IACA,GACAqB,IAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,IAAA,IACAF,IAAA,CAAAvB,IAAA;MACA,KAAA0B,IAAA,CAAAC,KAAA,cAAA3B,IAAA;MACA,IAAAA,IAAA,CAAA4B,QAAA;QACA,KAAAC,YAAA,CAAAC,OAAA;UACAC,OAAA;QACA;QACA,KAAAzB,gBAAA,GAAAN,IAAA,CAAA+B,OAAA;MACA;MACA,IAAA/B,IAAA,CAAA4B,QAAA;QACA,KAAAC,YAAA,CAAAG,KAAA;UACAD,OAAA,EAAA/B,IAAA,CAAA+B;QACA;MACA;IACA,GACAE