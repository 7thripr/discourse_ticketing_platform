{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport * as common from \"../assets/common.js\";\nexport default {\n  name: \"LoginView\",\n  components: {},\n  data() {\n    return {\n      form: {\n        email: \"\",\n        password: \"\"\n      },\n      show: true\n    };\n  },\n  methods: {\n    onSubmit: async function (event) {\n      event.preventDefault();\n      // alert('Do you want to login?');\n      this.flash(\"Internal Server Error\", \"success\");\n      this.flash(\"Internal Server Error\", \"error\");\n      this.flash(\"Internal Server Error\", \"info\");\n      this.flashMessage.error({\n        title: \"Error Message Title\",\n        message: \"Oh, you broke my heart! Shame on you!\"\n      });\n      this.flashMessage.warning({\n        title: \"Warning Message Title\",\n        message: \"Don't stop me nooooow....!\"\n      });\n      this.flashMessage.info({\n        title: \"Info Message Title\",\n        message: \"Just want you to know, that Vue is so cool\"\n      });\n      this.flashMessage.success({\n        title: \"Success Message Title\",\n        message: \"Hoorah, it is my fist npm package and it works!\"\n      });\n      fetch(common.BASEURL + common.AUTH_API_PREFIX + \"/login\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.form)\n      }).then(response => response.json()).then(data => {\n        console.log(\"Success:\", data);\n        if (data.category == \"success\") {\n          common.displayStatusSuccess(data.message);\n          this.$router.push(\"/student_home\");\n        }\n        if (data.category == \"error\") {\n          common.displayStatusError(data.message);\n        }\n      }).catch(error => {\n        console.error(\"Error:\", error);\n        // common.displayStatusError('Internal Server Error');\n        this.flash(\"Internal Server Error\", \"error\");\n      });\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.form.email = \"\";\n      this.form.password = \"\";\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    }\n  }\n};","map":{"version":3,"names":["common","name","components","data","form","email","password","show","methods","onSubmit","event","preventDefault","flash","flashMessage","error","title","message","warning","info","success","fetch","BASEURL","AUTH_API_PREFIX","method","headers","body","JSON","stringify","then","response","json","console","log","category","displayStatusSuccess","$router","push","displayStatusError","catch","onReset","$nextTick"],"sources":["src/views/LoginView.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin-top: 3%; margin-left: 3%; text-align: left\">\r\n    <h1 style=\"text-align: center\">Welcome to Online Support Ticket System</h1>\r\n    <FlashMessage></FlashMessage>\r\n    <div class=\"login-form\">\r\n      <h3 style=\"text-align: left\">Login</h3>\r\n      <br />\r\n      <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-email\"\r\n            v-model=\"form.email\"\r\n            type=\"email\"\r\n            placeholder=\"Enter email\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-password\"\r\n            v-model=\"form.password\"\r\n            placeholder=\"Enter password\"\r\n            type=\"password\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n        <br />\r\n        <b-button style=\"margin: 10px\" type=\"submit\" variant=\"primary\">Submit</b-button>\r\n        <b-button style=\"margin: 10px\" type=\"reset\" variant=\"danger\">Reset</b-button>\r\n      </b-form>\r\n      <br />\r\n      <b-link href=\"/register\">New user? Please Register here.</b-link>\r\n\r\n      <b-card class=\"mt-3\" header=\"Form Data : Temporary\">\r\n        <pre class=\"m-0\">{{ form }}</pre>\r\n      </b-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as common from \"../assets/common.js\";\r\n\r\nexport default {\r\n  name: \"LoginView\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      form: {\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n      show: true,\r\n    };\r\n  },\r\n  methods: {\r\n    onSubmit: async function (event) {\r\n      event.preventDefault();\r\n      // alert('Do you want to login?');\r\n      this.flash(\"Internal Server Error\", \"success\");\r\n      this.flash(\"Internal Server Error\", \"error\");\r\n      this.flash(\"Internal Server Error\", \"info\");\r\n      this.flashMessage.error({\r\n        title: \"Error Message Title\",\r\n        message: \"Oh, you broke my heart! Shame on you!\",\r\n      });\r\n      this.flashMessage.warning({\r\n        title: \"Warning Message Title\",\r\n        message: \"Don't stop me nooooow....!\",\r\n      });\r\n      this.flashMessage.info({\r\n        title: \"Info Message Title\",\r\n        message: \"Just want you to know, that Vue is so cool\",\r\n      });\r\n      this.flashMessage.success({\r\n        title: \"Success Message Title\",\r\n        message: \"Hoorah, it is my fist npm package and it works!\",\r\n      });\r\n\r\n      fetch(common.BASEURL + common.AUTH_API_PREFIX + \"/login\", {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(this.form),\r\n      })\r\n        .then((response) => response.json())\r\n        .then((data) => {\r\n          console.log(\"Success:\", data);\r\n          if (data.category == \"success\") {\r\n            common.displayStatusSuccess(data.message);\r\n            this.$router.push(\"/student_home\");\r\n          }\r\n          if (data.category == \"error\") {\r\n            common.displayStatusError(data.message);\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error:\", error);\r\n          // common.displayStatusError('Internal Server Error');\r\n          this.flash(\"Internal Server Error\", \"error\");\r\n        });\r\n    },\r\n    onReset(event) {\r\n      event.preventDefault();\r\n      // Reset our form values\r\n      this.form.email = \"\";\r\n      this.form.password = \"\";\r\n      // Trick to reset/clear native browser form validation state\r\n      this.show = false;\r\n      this.$nextTick(() => {\r\n        this.show = true;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.login-form {\r\n  width: 50%;\r\n  margin: 5%;\r\n}\r\n</style>\r\n"],"mappings":";AA0CA,YAAAA,MAAA;AAEA;EACAC,IAAA;EACAC,UAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAC,QAAA;MACA;MACAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,QAAA,iBAAAA,CAAAC,KAAA;MACAA,KAAA,CAAAC,cAAA;MACA;MACA,KAAAC,KAAA;MACA,KAAAA,KAAA;MACA,KAAAA,KAAA;MACA,KAAAC,YAAA,CAAAC,KAAA;QACAC,KAAA;QACAC,OAAA;MACA;MACA,KAAAH,YAAA,CAAAI,OAAA;QACAF,KAAA;QACAC,OAAA;MACA;MACA,KAAAH,YAAA,CAAAK,IAAA;QACAH,KAAA;QACAC,OAAA;MACA;MACA,KAAAH,YAAA,CAAAM,OAAA;QACAJ,KAAA;QACAC,OAAA;MACA;MAEAI,KAAA,CAAApB,MAAA,CAAAqB,OAAA,GAAArB,MAAA,CAAAsB,eAAA;QACAC,MAAA;QACAC,OAAA;UACA;QACA;QACAC,IAAA,EAAAC,IAAA,CAAAC,SAAA,MAAAvB,IAAA;MACA,GACAwB,IAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,IAAA,IACAF,IAAA,CAAAzB,IAAA;QACA4B,OAAA,CAAAC,GAAA,aAAA7B,IAAA;QACA,IAAAA,IAAA,CAAA8B,QAAA;UACAjC,MAAA,CAAAkC,oBAAA,CAAA/B,IAAA,CAAAa,OAAA;UACA,KAAAmB,OAAA,CAAAC,IAAA;QACA;QACA,IAAAjC,IAAA,CAAA8B,QAAA;UACAjC,MAAA,CAAAqC,kBAAA,CAAAlC,IAAA,CAAAa,OAAA;QACA;MACA,GACAsB,KAAA,CAAAxB,KAAA;QACAiB,OAAA,CAAAjB,KAAA,WAAAA,KAAA;QACA;QACA,KAAAF,KAAA;MACA;IACA;IACA2B,QAAA7B,KAAA;MACAA,KAAA,CAAAC,cAAA;MACA;MACA,KAAAP,IAAA,CAAAC,KAAA;MACA,KAAAD,IAAA,CAAAE,QAAA;MACA;MACA,KAAAC,IAAA;MACA,KAAAiC,SAAA