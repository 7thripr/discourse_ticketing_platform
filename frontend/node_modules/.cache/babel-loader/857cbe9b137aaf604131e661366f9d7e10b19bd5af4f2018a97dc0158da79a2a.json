 </b-col>\r\n        <b-col class=\"col\" cols=\"12\" sm=\"2\" lg=\"1\">\r\n          <b-row class=\"row\">\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\">\r\n              <b-button\r\n                @click=\"upvoteTicket\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"upvote_disabled\"\r\n                v-show=\"upvote_disabled ? !upvote_disabled : !(user_id==created_by)\"\r\n                ><b-icon\r\n                  icon=\"heart-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"success\"\r\n                ></b-icon>\r\n              </b-button>\r\n            </b-col>\r\n\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\"\r\n              ><b-button\r\n                @click=\"showEditTicketModal\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"edit_disabled\"\r\n                v-show=\"!edit_disabled\"\r\n                ><b-icon\r\n                  icon=\"pencil-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"primary\"\r\n                ></b-icon\r\n              ></b-button>\r\n            </b-col>\r\n\r\n            <b-col class=\"col\" cols=\"4\" sm=\"12\" lg=\"12\"\r\n              ><b-button\r\n                @click=\"showDeleteTicketModal\"\r\n                variant=\"outline-light\"\r\n                size=\"sm\"\r\n                class=\"ticket-card-buttons\"\r\n                :disabled=\"delete_disabled\"\r\n                v-show=\"!delete_disabled\"\r\n                ><b-icon\r\n                  icon=\"trash-fill\"\r\n                  aria-hidden=\"true\"\r\n                  class=\"bg-light\"\r\n                  variant=\"danger\"\r\n       