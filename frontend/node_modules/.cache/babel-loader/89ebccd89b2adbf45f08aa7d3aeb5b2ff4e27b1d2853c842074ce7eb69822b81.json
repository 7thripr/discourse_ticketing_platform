{"ast":null,"code":"export default {\n  name: \"RegisterView\",\n  components: {},\n  data() {\n    return {\n      role_options: [{\n        text: \"Student\",\n        value: \"student\"\n      }, {\n        text: \"Support\",\n        value: \"support\"\n      }, {\n        text: \"Admin\",\n        value: \"admin\"\n      }],\n      form: {\n        first_name: \"\",\n        last_name: \"\",\n        role: \"student\",\n        email: \"\",\n        password: \"\",\n        retype_password: \"\"\n      },\n      show: true\n    };\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      alert(JSON.stringify(this.form));\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.form.email = \"\";\n      this.form.password = \"\";\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    }\n  },\n  computed: {\n    check_name() {\n      return this.form.first_name.length > 2 ? true : false;\n    }\n  }\n};","map":{"version":3,"names":["name","components","data","role_options","text","value","form","first_name","last_name","role","email","password","retype_password","show","methods","onSubmit","event","preventDefault","alert","JSON","stringify","onReset","$nextTick","computed","check_name","length"],"sources":["src/views/RegisterView.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin-top: 3%; margin-left: 3%; text-align: left\">\r\n    <h1 style=\"text-align: center\">Welcome to Online Support Ticket System</h1>\r\n    <div class=\"register-form\">\r\n      <h3 style=\"text-align: left\">Register</h3>\r\n      <br />\r\n      <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-first-name\"\r\n            v-model=\"form.first_name\"\r\n            type=\"text\"\r\n            placeholder=\"Enter first name\"\r\n            :state=\"check_name\"\r\n            required\r\n          ></b-form-input>\r\n          \r\n        </b-form-group>\r\n\r\n        <!-- This will only be shown if the preceding input has an invalid state -->\r\n    <b-form-invalid-feedback id=\"input-live-feedback-first-name\">\r\n      Enter at least 3 letters of first name\r\n    </b-form-invalid-feedback>\r\n\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-last-name\"\r\n            v-model=\"form.last_name\"\r\n            type=\"text\"\r\n            placeholder=\"Enter last name\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n\r\n        <b-form-group label=\"Select role:\" v-slot=\"{ ariaDescribedby }\">\r\n          <b-form-radio-group\r\n            id=\"radio-group-role\"\r\n            v-model=\"form.role\"\r\n            :options=\"role_options\"\r\n            :aria-describedby=\"ariaDescribedby\"\r\n            name=\"radio-group-role\"\r\n          ></b-form-radio-group>\r\n        </b-form-group>\r\n\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-email\"\r\n            v-model=\"form.email\"\r\n            type=\"email\"\r\n            placeholder=\"Enter email\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-password\"\r\n            v-model=\"form.password\"\r\n            placeholder=\"Enter password\"\r\n            type=\"password\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n\r\n        <b-form-group\r\n          ><b-form-input\r\n            id=\"input-retype-password\"\r\n            v-model=\"form.retype_password\"\r\n            placeholder=\"Retype password\"\r\n            type=\"password\"\r\n            required\r\n          ></b-form-input\r\n        ></b-form-group>\r\n\r\n        <br />\r\n        <b-button style=\"margin: 10px\" type=\"submit\" variant=\"primary\">Submit</b-button>\r\n        <b-button style=\"margin: 10px\" type=\"reset\" variant=\"danger\">Reset</b-button>\r\n      </b-form>\r\n      <br />\r\n      <b-link href=\"/login\">Already registered? Please login here.</b-link>\r\n\r\n      <b-card class=\"mt-3\" header=\"Form Data : Temporary\">\r\n        <pre class=\"m-0\">{{ form }}</pre>\r\n      </b-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"RegisterView\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      role_options: [\r\n        { text: \"Student\", value: \"student\" },\r\n        { text: \"Support\", value: \"support\" },\r\n        { text: \"Admin\", value: \"admin\" },\r\n      ],\r\n      form: {\r\n        first_name: \"\",\r\n        last_name: \"\",\r\n        role: \"student\",\r\n        email: \"\",\r\n        password: \"\",\r\n        retype_password: \"\",\r\n      },\r\n      show: true,\r\n    };\r\n  },\r\n  methods: {\r\n    onSubmit(event) {\r\n      event.preventDefault();\r\n      alert(JSON.stringify(this.form));\r\n    },\r\n    onReset(event) {\r\n      event.preventDefault();\r\n      // Reset our form values\r\n      this.form.email = \"\";\r\n      this.form.password = \"\";\r\n      // Trick to reset/clear native browser form validation state\r\n      this.show = false;\r\n      this.$nextTick(() => {\r\n        this.show = true;\r\n      });\r\n    },\r\n    \r\n  },\r\n  computed: {\r\n    check_name(){\r\n      return this.form.first_name.length > 2 ? true : false\r\n    },\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.register-form {\r\n  width: 50%;\r\n  margin: 5%;\r\n}\r\n</style>\r\n"],"mappings":"AAyFA;EACAA,IAAA;EACAC,UAAA;EACAC,KAAA;IACA;MACAC,YAAA,GACA;QAAAC,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,EACA;MACAC,IAAA;QACAC,UAAA;QACAC,SAAA;QACAC,IAAA;QACAC,KAAA;QACAC,QAAA;QACAC,eAAA;MACA;MACA