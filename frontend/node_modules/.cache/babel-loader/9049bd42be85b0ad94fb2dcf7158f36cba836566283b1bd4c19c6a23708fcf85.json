input\"].files) {\r\n        //   formData.append('files', file, file.name) // note, no square-brackets\r\n        // }\r\n        for (let i = 0; i < this.form.dummy_file.length; i++) {\r\n          formData.append(`file_${i}`, this.form.dummy_file[i], this.form.dummy_file[i].name);\r\n        }\r\n\r\n        // this.formData.append('form', JSON.stringify(this.form)); //JSON.stringify(this.form)\r\n\r\n        for (var pair of formData.entries()) {\r\n          console.log(pair[0] + \", \" + pair[1]);\r\n        }\r\n\r\n        fetch(common.TICKET_API + `/${this.$store.getters.get_user_id}`, {\r\n          method: \"POST\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            web_token: this.$store.getters.get_web_token,\r\n            user_id: this.$store.getters.get_user_id,\r\n          },\r\n          body: JSON.stringify(this.form),\r\n        })\r\n          .then((response) => response.json())\r\n          .then((data) => {\r\n            this.$log.debug(`Success : ${data}`);\r\n            if (data.category == \"success\") {\r\n              this.flashMessage.success({\r\n                message: data.message,\r\n              });\r\n              this.onReset();\r\n            }\r\n            if (data.category == \"error\") {\r\n              this.flashMessage.error({\r\n                message: data.message,\r\n              });\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            this.$log.debug(`Error : ${error}`);\r\n            this.flashMessage.error({\r\n              message: \"Internal Server Error\",\r\n            });\r\n          });\r\n      }\r\n    },\r\n    onReset(event) {\r\n      event.preventDefault();\r\n      this.form.title = \"\";\r\n      this.form.description = \"\";\r\n      this.form.attachments = [];\r\n      this.$refs[\"file-input\"].reset();\r\n      // Trick to reset/clear native browser form validation state\r\n      this.show = false;\r\n      this.$nextTick(() => {\r\n        this.show = true;\r\n      });\r\n    },\r\n    onTagsChanged(value) {\r\n      this.form.tags = value;\r\n    },\r\n  },\r\n  computed: {\r\n    check_title() {\r\n      return this.form.title.length > 20 ? true : false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":";AAoFA,YAAAA,MAAA;AACA,OAAAC,UAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAF,OAAA;IAAAD;EAAA;EACAI,KAAA;IACA;MACAC,gBAAA,GACA;QAAAC,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,EACA;MACAC,IAAA;QACAC,KAAA;QACAC,WAAA;QACAC,QAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;QACAC,KAAA;QACAC,WAAA;MACA;MACAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAAZ,KAAA;MACA,KAAAC,IAAA,CAAAQ,WAAA,CAAAI,MAAA,SAAAZ,IAAA,CAAAQ,WAAA,CAAAK,MAAA,KAAAd,KAAA;MACA,SAAAe,CAAA,MAAAA,CAAA,QAAAd,IAAA,CAAAQ,WAAA,CAAAK,MAAA,EAAAC,CAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,CAAA,OAAAd,IAAA,CAAAQ,WAAA,CAAAM,CAAA,EAAAG,KAAA;MACA;IACA;IACAC,WAAAC,KAAA;MACA;MACA;MACA;MACA;MACA,SAAAL,CAAA,MAAAA,CAAA,GAAAK,KAAA,CAAAN,MAAA,EAAAC,CAAA;QACA,KAAAd,IAAA,CAAAoB,UAAA,CAAAC,IAAA,CAAAF,KAAA,CAAAL,CAAA;MACA;MACA;;MAEAC,OAAA,CAAAC,GAAA,uBAAAhB,IAAA,CAAAoB,UAAA,EAAAD,KAAA;IACA;IACAG,SAAAC,KAAA;MACAA,KAAA,CAAAC,cAAA;MAEA,SAAAxB,IAAA,CAAAI,IAAA,CAAAS,MAAA;QACAY,KAAA;MACA;QACAA,KAAA;QACA,KAAAC,IAAA,CAAAC,IAAA;QAIA,SAAAb,CAAA,SAAAd,IAAA,CAAAI,IAAA;UACA,SAAAJ,IAAA,CAAAI,IAAA,CAAAU,CAAA;YACA,KAAAd,IAAA,QAAA4B,QAAA,CAAAd,CAAA,gBAAAd,IAAA,CAAAI,IAAA,CAAAU,CAAA;UACA;QACA;QACAC,OAAA,CAAAC,GAAA,CAAAa,IAAA,CAAAC,SAAA,MAAA9B,IAAA;QAEAe,OAAA,CAAAC,GAAA,MAAAe,KAAA;QAEA,IAAAC,QAAA,OAAAC,QAAA;QACA;QACA;QACA;QACA,SAAAnB,CAAA,MAAAA,CAAA,QAAAd,IAAA,CAAAoB,UAAA,CAAAP,MAAA,EAAAC,CAAA;UACAkB,QAAA,CAAAE,MAAA,SAAApB,CAAA,SAAAd,IAAA,CAAAoB,UAAA,CAAAN,CAAA,QAAAd,IAAA,CAAAoB,UAAA,CAAAN,CAAA,EAAApB,IAAA;QACA;;QAEA;;QAEA,SAAAyC,IAAA,IAAAH,QAAA,CAAAI,OAAA;UACArB,OAAA,CAAAC,GAAA,CAAAmB,IAAA,aAAAA,IAAA;QACA;QAEAE,KAAA,CAAA9C,MAAA,CAAA+C,UAAA,YAAAC,MAAA,CAAAC,OAAA,CAAAC,WAAA;UACAC,MAAA;UACAC,OAAA;YACA;YACAC,SAAA,OA