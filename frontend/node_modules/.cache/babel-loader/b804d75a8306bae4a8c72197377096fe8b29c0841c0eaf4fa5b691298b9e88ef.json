{"ast":null,"code":"import * as common from \"../assets/common.js\";\nimport FileUpload from \"./FileUpload.vue\";\nimport Tagging from \"./Tagging.vue\";\nexport default {\n  name: \"FAQForm\",\n  props: [\"FAQ_id\", \"question\", \"solution\", \"tags\", \"hideReset\", \"editFAQ\"],\n  components: {\n    Tagging,\n    FileUpload\n  },\n  data() {\n    return {\n      form: {\n        question: this.question ? this.question : \"\",\n        solution: this.solution ? this.solution : \"\",\n        tags: [],\n        tag_1: \"\",\n        tag_2: \"\",\n        tag_3: \"\",\n        attachments: [],\n        created_by: \"\"\n      },\n      show: true,\n      user_id: this.$store.getters.get_user_id\n    };\n  },\n  created() {\n    console.log(`hide reset: ${this.hideReset}`);\n  },\n  methods: {\n    onFileUpload(value) {\n      this.form.attachments.splice(0, this.form.attachments.length, ...value);\n      for (let i = 0; i < this.form.attachments.length; i++) {\n        console.log(i, this.form.attachments[i].attachment_loc.slice(0, 50));\n      }\n    },\n    onSubmit(event) {\n      if (event && event.preventDefault) {\n        event.preventDefault();\n      }\n      if (this.form.tags.length == 0 && this.check_title && this.check_solution) {\n        alert(\"Choose atleast 1 tag, question should be atleast 5 characters long and solution should be 5 character long.\");\n      } else {\n        alert('Submitting form. Click \"Ok\" to proceed?');\n        this.$log.info(\"Submitting faq form\");\n        for (let i in this.form.tags) {\n          if (this.form.tags[i]) {\n            this.form[`tag_${parseInt(i) + 1}`] = this.form.tags[i];\n          }\n        }\n        // console.log(JSON.stringify(this.form));\n\n        // console.log(this.$refs);\n\n        this.form.created_by = this.user_id.toString();\n        console.log(this.$store.getters.get_web_token);\n        fetch(common.FAQ_API, {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            web_token: this.$store.getters.get_web_token,\n            user_id: this.user_id\n          },\n          body: JSON.stringify(this.form)\n        }).then(response => response.json()).then(data => {\n          this.$log.debug(`Success : ${data}`);\n          if (data.category == \"success\") {\n            this.flashMessage.success({\n              message: data.message\n            });\n          }\n          if (data.category == \"error\") {\n            this.flashMessage.error({\n              message: data.message\n            });\n          }\n        }).catch(error => {\n          this.$log.debug(`Error : ${error}`);\n          this.flashMessage.error({\n            message: \"Internal Server Error\"\n          });\n        });\n      }\n    },\n    onReset(event) {\n      if (event && event.preventDefault) {\n        event.preventDefault();\n      }\n\n      // event.preventDefault();\n      this.form.title = \"\";\n      this.form.description = \"\";\n      this.form.attachments = [];\n      this.form.tags = [];\n      // this.$refs[\"file-input\"].reset();\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    onTagsChanged(value) {\n      this.form.tags = value;\n    }\n  },\n  computed: {\n    check_title() {\n      console.log(this.form.title);\n      return this.form.question.length > 5 ? true : false;\n    },\n    check_solution() {\n      return this.form.solution.length > 5 ? true : false;\n    }\n  }\n};","map":{"version":3,"names":["common","FileUpload","Tagging","name","props","components","data","form","question","solution","tags","tag_1","tag_2","tag_3","attachments","created_by","show","user_id","$store","getters","get_user_id","created","console","log","hideReset","methods","onFileUpload","value","splice","length","i","attachment_loc","slice","onSubmit","event","preventDefault","check_title","check_solution","alert","$log","info","parseInt","toString","get_web_token","fetch","FAQ_API","method","headers","web_token","body","JSON","stringify","then","response","json","debug","category","flashMessage","success","message","error","catch","onReset","title","description","$nextTick","onTagsChanged","computed"],"sources":["src/components/FAQForm.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"faq-form\"\r\n    style=\"margin-top: 5px; margin-left: 5px; margin-right: 5px; text-align: left\"\r\n  >\r\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n      <b-form-group\r\n        ><b-form-input\r\n          id=\"input-question\"\r\n          v-model=\"form.question\"\r\n          type=\"text\"\r\n          placeholder=\"Enter Question\"\r\n          :state=\"check_title\"\r\n          aria-describedby=\"input-live-feedback-title\"\r\n          required\r\n        ></b-form-input>\r\n        <b-form-invalid-feedback id=\"input-live-feedback-title\">\r\n          Question should be atleast 5 characters long.\r\n        </b-form-invalid-feedback>\r\n      </b-form-group>\r\n\r\n      <b-form-group\r\n        ><b-form-textarea\r\n          id=\"input-solution\"\r\n          v-model=\"form.solution\"\r\n          type=\"text\"\r\n          placeholder=\"Enter Solution\"\r\n          :state=\"check_solution\"\r\n          rows=\"3\"\r\n          max-rows=\"6\"\r\n        ></b-form-textarea>\r\n        <b-form-invalid-feedback id=\"input-live-feedback-title\">\r\n          Solution should be atleast 5 characters long.\r\n        </b-form-invalid-feedback>\r\n      </b-form-group>\r\n\r\n      <Tagging @tags_changed=\"onTagsChanged\"></Tagging>\r\n\r\n      <FileUpload @file_uploading=\"onFileUpload\"></FileUpload>\r\n\r\n      <br />\r\n\r\n      <br />\r\n      <b-button style=\"margin: 10px\" type=\"submit\" variant=\"primary\">Submit</b-button>\r\n      <b-button v-show=\"hideReset ? false : true\" style=\"margin: 10px\" type=\"reset\" variant=\"danger\"\r\n        >Reset</b-button\r\n      >\r\n    </b-form>\r\n    <br />\r\n\r\n    <!-- <b-card class=\"mt-3\" header=\"Form Data : Temporary\">\r\n      <pre class=\"m-0\">{{ form }}</pre>\r\n    </b-card> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as common from \"../assets/common.js\";\r\nimport FileUpload from \"./FileUpload.vue\";\r\nimport Tagging from \"./Tagging.vue\";\r\n\r\nexport default {\r\n  name: \"FAQForm\",\r\n  props: [\"FAQ_id\", \"question\", \"solution\", \"tags\", \"hideReset\", \"editFAQ\"],\r\n  components: { Tagging, FileUpload },\r\n  data() {\r\n    return {\r\n      form: {\r\n        question: this.question ? this.question : \"\",\r\n        solution: this.solution ? this.solution : \"\",\r\n        tags: [],\r\n        tag_1: \"\",\r\n        tag_2: \"\",\r\n        tag_3: \"\",\r\n        attachments: [],\r\n        created_by: \"\",\r\n      },\r\n      show: true,\r\n      user_id: this.$store.getters.get_user_id,\r\n    };\r\n  },\r\n  created() {\r\n    console.log(`hide reset: ${this.hideReset}`);\r\n  },\r\n  methods: {\r\n    onFileUpload(value) {\r\n      this.form.attachments.splice(0, this.form.attachments.length, ...value);\r\n      for (let i = 0; i < this.form.attachments.length; i++) {\r\n        console.log(i, this.form.attachments[i].attachment_loc.slice(0, 50));\r\n      }\r\n    },\r\n    onSubmit(event) {\r\n      if (event && event.preventDefault) {\r\n        event.preventDefault();\r\n      }\r\n\r\n      if (this.form.tags.length == 0 && this.check_title && this.check_solution) {\r\n        alert(\r\n          \"Choose atleast 1 tag, question should be atleast 5 characters long and solution should be 5 character long.\"\r\n        );\r\n      } else {\r\n        alert('Submitting form. Click \"Ok\" to proceed?');\r\n        this.$log.info(\"Submitting faq form\");\r\n\r\n        for (let i in this.form.tags) {\r\n          if (this.form.tags[i]) {\r\n            this.form[`tag_${parseInt(i) + 1}`] = this.form.tags[i];\r\n          }\r\n        }\r\n        // console.log(JSON.stringify(this.form));\r\n\r\n        // console.log(this.$refs);\r\n\r\n        this.form.created_by = this.user_id.toString();\r\n        console.log(this.$store.getters.get_web_token);\r\n\r\n        fetch(common.FAQ_API, {\r\n          method: \"POST\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            web_token: this.$store.getters.get_web_token,\r\n            user_id: this.user_id,\r\n          },\r\n          body: JSON.stringify(this.form),\r\n        })\r\n          .then((response) => response.json())\r\n          .then((data) => {\r\n            this.$log.debug(`Success : ${data}`);\r\n            if (data.category == \"success\") {\r\n              this.flashMessage.success({\r\n                message: data.message,\r\n              });\r\n            }\r\n            if (data.category == \"error\") {\r\n              this.flashMessage.error({\r\n                message: data.message,\r\n              });\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            this.$log.debug(`Error : ${error}`);\r\n            this.flashMessage.error({\r\n              message: \"Internal Server Error\",\r\n            });\r\n          });\r\n      }\r\n    },\r\n    onReset(event) {\r\n      if (event && event.preventDefault) {\r\n        event.preventDefault();\r\n      }\r\n\r\n      // event.preventDefault();\r\n      this.form.title = \"\";\r\n      this.form.description = \"\";\r\n      this.form.attachments = [];\r\n      this.form.tags = [];\r\n      // this.$refs[\"file-input\"].reset();\r\n      // Trick to reset/clear native browser form validation state\r\n      this.show = false;\r\n      this.$nextTick(() => {\r\n        this.show = true;\r\n      });\r\n    },\r\n    onTagsChanged(value) {\r\n      this.form.tags = value;\r\n    },\r\n  },\r\n  computed: {\r\n    check_title() {\r\n      console.log(this.form.title);\r\n      return this.form.question.length > 5 ? true : false;\r\n    },\r\n    check_solution() {\r\n      return this.form.solution.length > 5 ? true : false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAyDA,YAAAA,MAAA;AACA,OAAAC,UAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;EACAC,UAAA;IAAAH,OAAA;IAAAD;EAAA;EACAK,KAAA;IACA;MACAC,IAAA;QACAC,QAAA,OAAAA,QAAA,QAAAA,QAAA;QACAC,QAAA,OAAAA,QAAA,QAAAA,QAAA;QACAC,IAAA;QACAC,KAAA;QACAC,KAAA;QACAC,KAAA;QACAC,WAAA;QACAC,UAAA;MACA;MACAC,IAAA;MACAC,OAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC;IACA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,qBAAAC,SAAA;EACA;EACAC,OAAA;IACAC,aAAAC,KAAA;MACA,KAAApB,IAAA,CAAAO,WAAA,CAAAc,MAAA,SAAArB,IAAA,CAAAO,WAAA,CAAAe,MAAA,KAAAF,KAAA;MACA,SAAAG,CAAA,MAAAA,CAAA,QAAAvB,IAAA,CAAAO,WAAA,CAAAe,MAAA,EAAAC,CAAA;QACAR,OAAA,CAAAC,GAAA,CAAAO,CAAA,OAAAvB,IAAA,CAAAO,WAAA,CAAAgB,CAAA,EAAAC,cAAA,CAAAC,KAAA;MACA;IACA;IACAC,SAAAC,KAAA;MACA,IAAAA,KAAA,IAAAA,KAAA,CAAAC,cAAA;QACAD,KAAA,CAAAC,cAAA;MACA;MAEA,SAAA5B,IAAA,CAAAG,IAAA,CAAAmB,MAAA,cAAAO,WAAA,SAAAC,cAAA;QACAC,KAAA,CACA,8GACA;MACA;QACAA,KAAA;QACA,KAAAC,IAAA,CAAAC,IAAA;QAEA,SAAAV,CAAA,SAAAvB,IAAA,CAAAG,IAAA;UACA,SAAAH,IAAA,CAAAG,IAAA,CAAAoB,CAAA;YACA,KAAAvB,IAAA,QAAAkC,QAAA,CAAAX,CAAA,gBAAAvB,IAAA,CAAAG,IAAA,CAAAoB,CAAA;UACA;QACA;QACA;;QAEA;;QAEA,KAAAvB,IAAA,CAAAQ,UAAA,QAAAE,OAAA,CAAAyB,QAAA;QACApB,OAAA,CAAAC,GAAA,MAAAL,MAAA,CAAAC,OAAA,CAAAwB,aAAA;QAEAC,KAAA,CAAA5C,MAAA,CAAA6C,OAAA;UACAC,MAAA;UACAC,OAAA;YACA;YACAC,SAAA,OAAA9B,MAAA,CAAAC,OAAA,CAAAwB,aAAA;YACA1B,OAAA,OAAAA;UACA;UACAgC,IAAA,EAAAC,IAAA,CAAAC,SAAA,MAAA5C,IAAA;QACA,GACA6C,IAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,IAAA,IACAF,IAAA,CAAA9C,IAAA;UACA,KAAAiC,IAAA,CAAAgB,KAAA,cAAAjD,IAAA;UACA,IAAAA,IAAA,CAAAkD,QAAA;YACA,KAAAC,YAAA,CAAAC,OAAA;cACAC,OAAA,EAAArD,IAAA,CAAAqD;YACA;UACA;UACA,IAAArD,IAAA,CAAAkD,QAAA;YACA,KAAAC,YAAA,CAAAG,KAAA;cACAD,OAAA,EAAArD,IAAA,CAAAqD;YACA;UACA;QACA,GACAE,KAAA,CAAAD,KAAA;UACA,KAAArB,IAAA,CAAAgB,KAAA,YAAAK,KAAA;UAC